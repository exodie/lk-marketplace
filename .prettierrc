{
  "printWidth": 88,
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "singleQuote": true,
  "trailingComma": "none",
  "bracketSpacing": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react",
    "^react-dom/*",
    "^react-router-dom",
    "^./(.*).css",
    "^../(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^@/components/*",
    "^@/components/ui/*",
    "^@/lib/*",
    "^@/hooks/*"
  ],
  "importOrderSeparation": true
}
